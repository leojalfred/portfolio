{
  "author": "Leo Alfred <leojalfred@gmail.com> (http://leojalfred.com/)",
  "browserslist": {
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ]
  },
  "dependencies": {
    "animated-scroll-to": "^2.2.0",
    "hamburger-react": "^2.5.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-scripts": "5.0.1",
    "sass": "^1.50.1"
  },
  "description": "Leo Alfred's personal portfolio website",
  "devDependencies": {
    "http-server": "^0.12.3"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true
    },
    "extends": [
      "react-app"
    ]
  },
  "license": "UNLICENSED",
  "name": "portfolio",
  "scripts": {
    "begin": "npm run build && npm run move",
    "build": "(if exist build rmdir /q /s build) && react-scripts build",
    "deploy": "npm run begin && cd ../production && git add . && git commit -m \"Deploy\" && git push origin master",
    "move": "(robocopy build ../production /s /purge /xf CNAME /xd .git) & exit 0",
    "production": "npm run begin && http-server ../production",
    "start": "react-scripts start",
    "tunnel": "ngrok http --host-header=rewrite 3000"
  },
  "version": "1.1.0"
}
